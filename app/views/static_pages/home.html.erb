<% if is_logged_in? %>
  <div class="row">
    <aside class="col-md-4">
      <section class="user_info">
        <%= image_tag "horse", alt: "#{current_user.name}", id: 'profile_photo' %>
      </section>
    </aside>
    <div class="col-md-8">
      <section class="feed">
        
      </section>
    </div>
  </div>
<% else %>
  <div class="jumbotron" id="home-banner">
    <div class="container">
      <h1 class="page-header">Welcome to RSN</h1>
      <p>
        This is a social network created by me. try it now!
      </p>
      <p>
        <%= link_to "Sign up", signup_path, class: 'btn btn-lg btn-primary' %>
      </p>
    </div>
  </div>
<% end %>